name: Manual Build Workflow

on:
  workflow_dispatch:

jobs:
  run-command:
    runs-on: ubuntu-latest

    steps:
    - name: Checkout Repository
      uses: actions/checkout@v2

    - name: Run a Command
      run: |
        echo "Hello, this is a manually triggered action!"
        # Replace the following line with your command
        your-command-here
